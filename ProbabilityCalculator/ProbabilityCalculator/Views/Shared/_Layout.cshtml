        <div class="navbar-header">
            <ul class="nav">
                <li>
                    <a href="/">Home</a>
                </li>
                <li>
                    <a href="/Home/About">About</a>
                </li>
            </ul>
        </div>
        </div>
    </nav>
    <div class="container">
    @RenderBody()
    </div>
    <footer class="">
        
    </footer>
    @Scripts.Render("~/bundles/jquery")
